{
  "redirects": [
    {
      "source": "/:shortUrl",
      "has": [
        {
          "type": "host",
          "value": "slashbyhash.vercel.app"
        }
      ],
      "destination": "https://url-shortener-backend-0bgv.onrender.com/:shortUrl",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/:shortUrl((?!api|login|register|dashboard|home)[a-zA-Z0-9_-]{5,})",
      "destination": "https://url-shortener-backend-0bgv.onrender.com/:shortUrl"
    },
    {
      "source": "/api/:path*",
      "destination": "https://url-shortener-backend-0bgv.onrender.com/api/:path*"
    }
  ]
}
