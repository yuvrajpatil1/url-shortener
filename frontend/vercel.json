{
  "rewrites": [
    {
      "source": "/:shortUrl",
      "has": [
        {
          "type": "host",
          "value": "slashbyhash.vercel.app"
        },
        {
          "type": "missing",
          "value": "/(login|register|delete|dashboard|profile)"
        }
      ],
      "destination": "https://url-shortener-backend-0bgv.onrender.com/:shortUrl"
    },
    {
      "source": "/register",
      "destination": "/index.html"
    },
    {
      "source": "/login",
      "destination": "/index.html"
    },
    {
      "source": "/delete",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
